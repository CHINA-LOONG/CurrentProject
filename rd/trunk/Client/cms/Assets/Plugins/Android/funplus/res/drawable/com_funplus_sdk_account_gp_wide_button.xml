        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <solid android:color="@color/com_funplus_sdk_account_gp_light_red"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <solid android:color="@color/com_funplus_sdk_account_gp_light_red"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <solid android:color="@color/com_funplus_sdk_account_gp_dark_red"/>
        </shape>
    </item>
</selector>